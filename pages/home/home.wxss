/* pages/home/home.wxss */
@font-face {
  font-family: 'iconfont';
  src: url('data:font/truetype;charset=utf-8;base64,') format('truetype');
  font-weight: normal;
  font-style: normal;
  font-display: swap;
}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.icon-xiaoxi:before {
  content: "\e6b0";
}
.van-tabs__nav,.van-tab{
  background-color: #bf0000;
}
.van-tabs__line{
  bottom: 5px !important;
}
.van-progress__pivot{
  right: -21px !important;
}
/* -------------------------- */
.message{
  text-align: center;
  font-size: 13px;
  line-height: 20px;
  color: #fff;
  display: flex;
  flex-direction: column;
}
.message_icon{
  font-size: 20px
}
/* ----------------------------- */
.container{
  background-color: #f6f6f6;
  padding: 10px;
}
.banner{
  width: 100%;
  border-radius: 10px;
  margin-bottom: 10px;
}
.banner swiper-item{
  border-radius: 10px;
}
.banner_image{
  border-radius: 10px;
}
/* ------------------ */
.home_cate{
  width: 100%;
}
.home_cate{
  background-color: #fff;
}
.cate_ul{
  width: 100%;
  display: flex;
  flex-wrap: wrap;
}
.cate_li{
  width: 20%;
  display: flex;
  flex-direction: column;
  text-align: center;
  margin-bottom: 10px;
  font-size: 14px;
}
.cate_li image{
  width: 50px;
  height: 50px;
  margin: 0 auto;
}
/* ---------------------- */
.notice{
  width: 100%;
  height: 40px;
  margin-bottom: 10px;
  background-color: #fff;
  display: flex;
  align-items: center;
}
.notice_left{
  width: 60px;
  height: 15px;
}
.notice_left image{
  width: inherit;
  height: inherit;
  margin-left: 10px;
}
.notice_right{
  width: 100%;
  height: 20px;
  margin-left: 30px;
}
.notice_list{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
}
.notice_item{
  display: flex;
}
.notice_item text{
  height: 20px;
  line-height: 20px;
  font-size: 13px;
  margin-left: 15px;
}
.notice_flag{
  width: 58px;
  height: 100%;
  border-radius: 20px;
  background-color: orange;
  color: #fff;
  text-align: center;
  font-size: 12px;
}
.notice_text{
  width: 167px;
  height: 100%;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
.notice_arrow{
  font-size: 20px;
}
/* -------------------- */
.recommend{
  width: 100%;
  height: 100%;
  background-color: #fff;
  display: flex;
  flex-direction: column;
  margin: 0 auto 10px;
}
.re_top{
  width: 100%;
  height: 90px;
}
.re_center{
  width: 100%;
  margin-top: 5px;
  display: flex;
  justify-content: space-between;
}
.re_center image{
  width: 180px;
  height: 80px;
}
.re_bottom{
  padding: 0 10px;
  margin-top: 5px;
  display: flex;
  justify-content: space-between;
}
.re_bottom image{
  width: 88px;
  height: 104px;
}
/* ------------------ */
.brand{
  width: 100%;
  height: 100%;
  background-color: #fff;
  margin-bottom: 10px;
}
.brand_top{
  width: 100%;
  display: flex;
  justify-content: space-between;
  margin-bottom: 5px;
  padding: 10px 0 6px;
}
.brand_title{
  padding: 0 10px;
}
.latest_goods{
  color: #696969;
  font-size: 12px;
}
.brand_message{
  width: 120px;
  height: 100%;
  display: flex;
  flex-direction: column;
  padding-top: 3px;
}
.brand_item{
  color: #fe3738;
  font-size: 12px;
  text-align: right;
}
.brand_arrow{
  color: #fe3738;
  font-size: 12px;
}
.brand_message_text{
  width: 100%;
  height: 100%;
}
.brand_name{
  width: 330px;
  height: 80px;
  background: url(https://img.alicdn.com/imgextra/i2/2053469401/O1CN01Tzr9Yl2JJi4BVzBMr_!!2053469401.jpg);background-size: 100%;
  border-radius: 10px;
  margin: 0 auto;
  padding: 10px 20px;
  font-size: 14px;
  color: #fff;
}
.brand_logo{
  display: flex;
  align-items: center;
  margin-bottom: 10px;
}
.brand_logo image{
  width: 30px;
  height: 30px;
  border-radius: 6px;
  margin-right: 5px;
}
.brand_list swiper{
  width: 100%;
  height: 280px;
}
.brand_detail{
  width: 100%;
  display: flex;
  justify-content: space-around;
}
.brand_detail_li{
  width: 33.3%;
  height: 145px;
  margin-top: 10px;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.brand_detail_li image{
  width: 105px;
  height: 105px;
  margin: 0 auto;
}
.ticket_border{
  font-size: 12px;
  border: 1px solid #ff5151;
  color: #ff5151;
}
.ticlet_value{
  background-color: #ff5151;
  color: #fff;
  font-size: 13px;
  border-bottom: 1px solid transparent;
  padding: 0 2px;
}
.goods_price{
  font-size: 15px;
  color: #fe3738;
}
.old_price{
  font-size: 12px;
  color: #bdbbbb;
  text-decoration:line-through;
}
/* ------------------ */
.weGet{
  width: 100%;
  height: 270px;
  background-color: #fff;
  margin-bottom: 10px;
}
.weGet_top{
  padding: 10px 10px 6px;
  display: flex;
  justify-content: space-between;
}
.weGet_top text{
  margin-left: 6px;
}
.weGet_swiper{
  width: 100%;
  height: 28px !important;
  margin: 5px 0;
}
.weGet_swiper swiper-item{
  width: 80% !important;
  height: 28px !important;
  line-height: 28px;
  margin-left: 50px;
  font-size: 13px;
  display: flex;
  align-items: center;
}
.weGet_swiper swiper-item image{
  width: 28px;
  height: 28px;
  border-radius: 50%;
  display: inline-block;
  overflow: hidden;
}
.weGet_swiper swiper-item text{
  width: 100%;
  height: 25px;
  padding-left: 5px;
  line-height: 25px;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  background-color: #ffebea;
  color: #fe3a33;
  border-radius: 20px;
}
.weGet_list{
  height: 200px !important;
  padding: 10px;
  display: flex;
  overflow-x: scroll;
}
.weGet_list_item{
  width: 40%;
  height: 100%;
  margin-right: 30px;
}
.weGet_list_item:last-child{
  margin-right: 0;
}
.weGet_list image{
  width: 105px;
  height: 105px;
}
.ticket_after{
  display: flex;
  align-items: center;
  margin-bottom: 5px;
}