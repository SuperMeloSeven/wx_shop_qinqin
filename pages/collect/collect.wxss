.goods_list{
  width: 100%;
  height: auto;
  padding: 10px 0;
  background-color: #f5f5f5;
}
.goods_detail{
  width: auto;
  height: auto;
  padding: 10px 0 10px 10px;
  background-color: #fff;
  border-radius: 10px;
  border-bottom: 1px solid #e0dfdf;
  display: flex;
}
.goods_img{
  width: 40%;
  height: 140px;
  border-radius: 5px;
}
.goods_container{
  width: 60%;
  margin-left: 15px;
  position: relative;
}
.goods_name{
  width: auto;
  margin-bottom: 30px;
  padding-right: 10px;
  font-size: 14px;
  overflow : hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.goods_name text{
  margin-left: 10px;
}
.label{
  font-size: 12px;
  margin: 5px 0;
  display: flex;
}
.ticket_border{
  font-size: 12px;
  border: 1px solid #ff5151;
  color: #ff5151;
}
.ticlet_value{
  background-color: #ff5151;
  color: #fff;
  font-size: 13px;
  border-bottom: 1px solid transparent;
  padding: 0 2px;
}
.sales_num{
  font-size: 13px;
  color: #666;
}
.shop{
  display: flex;
  font-size: 13px;
  color: #666;
}
.shop_name{
  margin-right: 5px;
}
.resemble{
  width: 85px;
  height: 30px;
  line-height: 30px;
  color: #fff;
  font-size: 15px;
  text-align: center;
  background: linear-gradient(to right, #fe5a33, #ef0f25);
  border-radius: 20px;
  position: absolute;
  right: 10px;
  bottom: 0;
}
.culling{
  color: #FC3F78;
  display: flex;
  justify-content: center;
  align-items: center;
}
.line{
  width: 60px;
  height: 1px;
  background-color: #FC3F78;
  margin: 0 5px;
}