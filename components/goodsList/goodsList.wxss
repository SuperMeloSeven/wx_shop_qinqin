.ticket_border{
  font-size: 12px;
  border: 1px solid #ff5151;
  color: #ff5151;
}
.ticlet_value{
  background-color: #ff5151;
  color: #fff;
  font-size: 13px;
  border-bottom: 1px solid transparent;
  padding: 0 2px;
}
/* -------------------- */
.goods_list{
  width: 100%;
  height: auto;
  padding: 10px 0;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}
.goods_detail{
  width: 49%;
  height: auto;
  padding-bottom: 10px;
  background-color: #fff;
  border-radius: 10px;
  margin-bottom: 10px;
  display: flex;
  flex-direction: column;
}
.goods_img{
  width: 100%;
  height: 173px;
  border-top-left-radius: 10px;
  border-top-right-radius: 10px;
}
.goods_container{
  padding: 0 5px;
}
.goods_name{
  margin-bottom: 10px;
  font-size: 14px;
  overflow : hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.goods_name text{
  margin-left: 10px;
}
.label{
  font-size: 12px;
  margin: 5px 0;
  display: flex;
}
.labelOne{
  padding: 0 2px;
  border: 1px solid orange;
  color: orange;
  margin-right: 5px;
  border-radius: 2px;
}
.labelTwo{
  border: 1px solid #ff0137;
  color: #ff0137;
  border-radius: 5px;
  margin-right: 5px;
  border-radius: 2px;
}
.sales_num{
  font-size: 12px;
  color: #666;
}